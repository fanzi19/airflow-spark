<configuration>
  <property>
    <name>ssl.client.keystore.location</name>
    <value>/etc/security/client.keystore</value>
  </property>
  <property>
    <name>ssl.client.keystore.password</name>
    <value>${ssl.client.keystore.password}</value>
  </property>
  <property>
    <name>ssl.client.truststore.location</name>
    <value>/etc/security/client.truststore</value>
  </property>
  <property>
    <name>ssl.client.truststore.password</name>
    <value>${ssl.client.truststore.password}</value>
  </property>
</configuration>
